<Window x:Class="WpfApp3.uslugi"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp3"
        mc:Ignorable="d"
        Title="uslugi" Height="600" Width="600" ResizeMode="NoResize" Closing="Window_Closed">
    <Grid>
        <Button x:Name="form_button" Content="Подать заявку на страхование" FontSize="38" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="574" Height="91" Click="form_button_Click"/>
        <Label x:Name="label_fio" Content="ФИО" Visibility="Hidden" FontSize="38" HorizontalAlignment="Left" Margin="244,195,0,0" VerticalAlignment="Top" Height="64" Width="100"/>
        <TextBox x:Name="fio" FontSize="38" Visibility="Hidden" HorizontalAlignment="Left" Height="82" Margin="35,298,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="523"/>
        <Button x:Name="send" Content="Отправить" FontSize="38" Visibility="Hidden" HorizontalAlignment="Left" Margin="35,436,0,0" VerticalAlignment="Top" Width="523" Height="75" Click="send_Click"/>

    </Grid>
</Window>
